# Azure OpenAI Configuration
# Copy this file to .env and fill in your actual values

# Azure OpenAI API Key (required)
AIM_OPENAI_KEY=your_api_key_here

# Azure OpenAI Endpoint (required)
AZURE_ENDPOINT=https://aim-australia-east.openai.azure.com/

# Azure Model Names
AZURE_MODEL_NAME=gpt-5-hiring
AZURE_MINI_MODEL_NAME=gpt-5-mini-hiring

# Azure API Version
AZURE_API_VERSION=2025-03-01-preview

# DSPy Temperature (0.0 to 2.0, default: 1.0)
DSPY_TEMPERATURE=1.0
